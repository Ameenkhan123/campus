				<ol class="carousel-indicators">
					<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
					<li data-target="#myCarousel" data-slide-to="1"></li>
					<li data-target="#myCarousel" data-slide-to="2"></li>
					<li data-target="#myCarousel" data-slide-to="3"></li>
					<li data-target="#myCarousel" data-slide-to="4"></li>			
				</ol>
				<!-- Wrapper for slides -->
				<div class="carousel-inner" role="listbox">
					<div class="item active">
						<%= image_tag 'ruby.jpg', :style => "width:1366px; height:300px", :class => "img-responsive image"%>
						<div class="carousel-caption hidden-xs">
							<!-- <h2>Ruby ON Rails</h2> -->
						</div>
					</div>
					<div class="item">
						<%= image_tag 'bootstrap.png', :style => "width:1366px; height:300px", :class => "img-responsive"%>
						<div class="carousel-caption hidden-xs">
							<!-- <h2>BootStrap</h2> -->
							<
						</div>
					</div>
					<div class="item ">
						<%= image_tag 'angularjs.png', :style => "width:1366px; height:300px", :class => "img-responsive"%>
						<div class="carousel-caption hidden-xs">
							<!-- <h2>AngularJs</h2> -->
						</div>
					</div>					
					<div class="item">
						<%= image_tag 'python.jpg', :style => "width:1366px; height:300px", :class => "img-responsive"%>
						<div class="carousel-caption hidden-xs">
							<!-- 	<h2>Python</h2> -->
						</div>
					</div>
					<div class="item">
						<%= image_tag 'Nodejs.png', :style => "width:1366px; height:300px", :class => "img-responsive"%>
						<div class="carousel-caption hidden-xs">
							<!-- <h2>Nodejs</h2> -->
						</div>
					</div>
				</div>
				<!-- Left and right controls -->
				<a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
					<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
					<span class="sr-only">Previous</span>
				</a>
				<a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
					<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
					<span class="sr-only">Next</span>
				</a>
			</div>
		</div>
		<span class="container">			
			<div class="col-md-4 well" style="border-radius:0px;
			box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);">
			<h4><i><b>Resgistered here for pool compus at PIEMR, 2018</b></i></h4>
